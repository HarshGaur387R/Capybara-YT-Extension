{
    "name": "Capybara Comments",
    "version": "0.1",
    "manifest_version": 3,
    "description": "Capybara Comments is a youtube video comments generator .",
    "icons": {
        "16": "icons/extension-icon-16.png",
        "48": "icons/extension-icon-48.png",
        "128": "icons/extension-icon-128.png"
    },
    "permissions": [
        "activeTab",
        "storage"
    ],
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": "icons/extension-icon-16.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}